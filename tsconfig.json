{
  "composite": true,
  "compilerOptions":{
    "outDir": "./build/types",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["chrome"],
    "typeRoots":["../webapp/types/next-auth.d.ts"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict":false,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl":".",
    "paths":{
      "@components/*":["components/*.tsx"],
      "@styles/*":["styles/*"],
      "@pages/*": ["pages/*.tsx"],
      "@lib/*": ["lib/*.ts",],
      "@context/*": ["context/*.tsx"],
      "@util/*": ["util/*tsx","util/*ts"],
      "@service/*": ["service/*.ts"],
      "@api/*": ["pages/api/*.ts"],
    }
    
  },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_document.tsx", "node_modules/Navbar.tsx","pages/**/*",
      "src/**/*", "interface/userSes.js", "next.config.js",],
    "exclude": ["node_modules"],
  
 
}
