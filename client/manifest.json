{
    "manifest_version":2,
    "name": "Shopply Product Searcher",
    "description": "Free analytic tool for jumia sellers in Nigeria",
    "version": "0.01", 
    "content_scripts":[
        {
            "js": ["js/app/context.js"],
            "matches":["<all_urls>"]
        }
    ],
    "background": {
        "scripts" : ["js/lib/jquery-3.6.0.min.js","js/app/background.js"]

    },
    "browser_action": {
        "default_popup":"views/popup.html", 
        "default_title": "Yapa Analaytcs",
        "default_icon":"icon.png"
    }, 
    "icons":{
        "16":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    }
    
}