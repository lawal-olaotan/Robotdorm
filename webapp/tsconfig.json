{
  "compilerOptions":{
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl":".",
    "paths":{
      "@components/*":["components/*.tsx"],
      "@styles/*":["styles/*"],
      "@pages/*": ["pages/*.tsx"]
    }
    
  },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_document.tsx", "node_modules/Navbar.tsx","pages/**/*",
      "src/**/*",],
    "exclude": ["node_modules"],
  
 
}
